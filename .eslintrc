{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "prettier/prettier": "error",
    "linebreak-style": 0,
    "no-restricted-syntax": 0
  }
}
